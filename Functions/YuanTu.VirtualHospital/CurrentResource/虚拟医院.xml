<?xml version="1.0" encoding="utf-8" ?>
<root>
  <!--医院编号(与前置网关参数对应[唯一])-->
  <HospitalId>9999</HospitalId>
  <!--操作员编号，建议每台自助机唯一-->
  <OperatorId>ZZJ001</OperatorId>
  <!--院区ID-->
  <HospitalAreaCode>3307820001</HospitalAreaCode>
  <!--机器型号，与自助机铭牌上一致-->
  <DeviceType>YT-540</DeviceType>
  <!--前置网关Url地址-->
  <GatewayUrl>http://ngrok.yuantutech.com:32091/frontgateway/gateway.do</GatewayUrl>

  <Functions>   
    <生物信息录入>
      <Name>生物信息录入</Name>
      <Order>11</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_建档发卡</ImageName>
    </生物信息录入>
    <!--双屏-->
    <建档>
      <Name>自助建档</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_建档发卡</ImageName>
    </建档>
    <挂号>
      <Name>挂号</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_挂号</ImageName>
    </挂号>
    <预约>
      <Name>预约</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_预约</ImageName>
    </预约>
    <取号>
      <Name>自助取号</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_取号</ImageName>
    </取号>
    <缴费>
      <Name>自助缴费</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_缴费</ImageName>
    </缴费>
    <充值>
      <Name>自助充值</Name>
      <Order>5</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_充值</ImageName>
    </充值>
    <住院押金>
      <Name>住院押金</Name>
      <Order>6</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_住院押金</ImageName>
    </住院押金>
    <出院结算>
      <Name>出院结算</Name>
      <Order>6</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_缴费</ImageName>
    </出院结算>
    <查询>
      <Name>自助查询</Name>
      <Order>7</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_查询</ImageName>
    </查询>
    <收银>
      <Name>收银</Name>
      <Order>0</Order>
      <Visabled>0</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_充值</ImageName>
    </收银>
    <先诊疗后付费>
      <Name>先诊疗后付费</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_充值</ImageName>
    </先诊疗后付费>
  </Functions>
  <!--办卡选项-->
  <CreateType>
    <成人>
      <Name>成人</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>办卡类型_成人</ImageName>
      <Color>137,212,164</Color>
    </成人>
    <儿童>
      <Name>儿童</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>办卡类型_儿童</ImageName>
      <Color>248,180,117</Color>
    </儿童>
  </CreateType>

  <Loan>
    <状态查询>
      <Name>状态查询</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_建档发卡</ImageName>
    </状态查询>
    <自助还款>
      <Name>自助还款</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_挂号</ImageName>
    </自助还款>
  </Loan>

  <!--可使用鉴定病人信息的卡片-->
  <Card>
    <身份证>
      <Name>身份证</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_身份证</ImageName>
      <建档>1</建档>
      <查询>1</查询>
    </身份证>
    <就诊卡>
      <Name>就诊卡</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_诊疗卡</ImageName>
      <建档>0</建档>
      <查询>1</查询>
    </就诊卡>
    <社保卡>
      <Name>社保卡</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_社保卡</ImageName>
      <建档>0</建档>
      <查询>1</查询>
    </社保卡>
    <扫码>
      <Name>扫码</Name>
      <Order>5</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_慧医扫码</ImageName>
      <建档>0</建档>
      <查询>1</查询>
      <住院>1</住院>
    </扫码>
    <刷脸>
      <Name>人脸识别</Name>
      <Order>6</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_人脸识别</ImageName>
      <建档>0</建档>
      <查询>1</查询>
      <住院>1</住院>
    </刷脸>
  </Card>
  <PayOut>
    <先诊疗后付费>
      <Name>先诊疗后付费</Name>
      <Order>10</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>支付_诊疗卡账户</ImageName>
      <Color>137,212,164</Color>
    </先诊疗后付费>
  </PayOut>
  <Repay>
    <预缴金>
      <Name>诊疗卡支付</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>支付_诊疗卡账户</ImageName>
      <Color>137,212,164</Color>
    </预缴金>
    <银联>
      <Name>银联支付</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
    <支付宝>
      <Name>支付宝支付</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>支付_支付宝</ImageName>
      <Color>116,206,230</Color>
      <Hint>随机奖励金</Hint>
      <Template>InfoItemPayBubble</Template>
    </支付宝>
    <微信支付>
      <Name>微信支付</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>支付_微信</ImageName>
      <Color>137,212,164</Color>
    </微信支付>
    <苹果支付>
      <Name>Apple Pay</Name>
      <Order>5</Order>
      <Visabled>1</Visabled>
      <IsEnabled>0</IsEnabled>
      <ImageName>支付_ApplePay</ImageName>
      <Color>162,169,239</Color>
    </苹果支付>
  </Repay>

  <!--所有打印机配置-->
  <Printer>
    <!--凭条打印机名称-->
    <Receipt>Microsoft XPS Document Writer</Receipt>
    <!--斑马打印机名称-->
    <ZBRPrinter>Zebra ZXP Series 3 USB Card Printer</ZBRPrinter>
    <!--激光打印机-->
    <Laser>Microsoft XPS Document Writer</Laser>
  </Printer>
</root>