<?xml version="1.0" encoding="utf-8" ?>
<root>
  <!--医院编号(与前置网关参数对应[唯一])-->
  <HospitalId>820</HospitalId>
  <!--操作员编号，建议每台自助机唯一-->
  <OperatorId>ZY0001</OperatorId>
  <!--操作员姓名，建议每台自助机唯一-->
  <OperatorName>远图自助机</OperatorName>
  <!--院区ID-->
  <HospitalAreaCode>A10001</HospitalAreaCode>
  <!--前置网关Url地址-->
  <GatewayUrl>http://ngrok.yuantutech.com:39900/frontgateway/gateway.do</GatewayUrl>
  <!--监控平台Url地址-->
  <MonitorUrl>http://121.40.253.119/ytmonitor/gateway.do</MonitorUrl>
  <!--机器型号，与自助机铭牌上一致-->
  <DeviceType>YT-540</DeviceType>

  <Assembly>YuanTu.HuNanHangTianHospital</Assembly>
  <DatabasePath>Data\Database.db</DatabasePath>

  <!--组播发送的IP地址与端口-->
  <MultiCastIP>224.0.200.200</MultiCastIP>
  <MultiCastPort>8888</MultiCastPort>

  <!--是否启用语音播报引导-->
  <AudioGuideEnabled>1</AudioGuideEnabled>

  <!--按钮反馈(目前支持点击后声音)-->
  <ButtonFeedBack>
    <Enable>1</Enable>
    <BeepFrequency>1000</BeepFrequency>
    <BeepDuration>200</BeepDuration>
  </ButtonFeedBack>
  <!--是否启用自动更新-->
  <AutoUpdate>0</AutoUpdate>

  <!--界面倒计时-->
  <TimeOut>60</TimeOut>
  <!--自动关机-->
  <AutoShutdown>
    <Enabled>0</Enabled>
    <Time>17:30</Time>
  </AutoShutdown>
  <!--办卡需要选择类型-->
  <SelectCreateType>0</SelectCreateType>

  <!--主页功能设置-->
  <Functions>
    <建档>
      <Name>自助建档</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_建档发卡</ImageName>
    </建档>
    <挂号>
      <Name>自助挂号</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_挂号</ImageName>
    </挂号>
    <预约>
      <Name>自助预约</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_预约</ImageName>
    </预约>
    <取号>
      <Name>自助取号</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_取号</ImageName>
    </取号>
    <缴费>
      <Name>自助缴费</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_缴费</ImageName>
    </缴费>
    <充值>
      <Name>自助充值</Name>
      <Order>5</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_充值</ImageName>
    </充值>
    <查询>
      <Name>自助查询</Name>
      <Order>7</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_查询</ImageName>
    </查询>
    <补打>
      <Name>自助补打</Name>
      <Order>8</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_补打</ImageName>
    </补打>
  </Functions>

  <!--查询功能-->
  <InfoQuery>
    <检验结果>
      <Name>检验结果查询</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_检查报告</ImageName>
    </检验结果>
    <已缴费明细>
      <Name>已缴费明细查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_门诊消费</ImageName>
    </已缴费明细>
  </InfoQuery>
  <!--补打功能-->
  <PrintAgain>
    <缴费补打>
      <Name>缴费补打</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_缴费补打</ImageName>
    </缴费补打>
  </PrintAgain>
  <!--办卡选项-->
  <CreateType>
    <成人>
      <Name>成人</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>办卡类型_成人</ImageName>
      <Color>137,212,164</Color>
    </成人>
    <儿童>
      <Name>儿童</Name>
      <Order>1</Order>
      <Visabled>0</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>办卡类型_儿童</ImageName>
      <Color>248,180,117</Color>
    </儿童>
  </CreateType>

  <!--可使用鉴定病人信息的卡片-->
  <Card>
    <身份证>
      <Name>身份证</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_身份证</ImageName>
      <建档>1</建档>
      <查询>0</查询>
    </身份证>
    <就诊卡>
      <Name>就诊卡</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_诊疗卡</ImageName>
      <建档>0</建档>
      <查询>1</查询>
    </就诊卡>
    <社保卡>
      <Name>社保卡</Name>
      <Order>2</Order>
      <Visabled>0</Visabled>
      <ImageName>卡_社保卡</ImageName>
      <建档>1</建档>
      <查询>1</查询>
    </社保卡>
  </Card>

  <!--可选的充值方式-->
  <Recharge>
    <银联>
      <Name>银联充值</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
  </Recharge>

  <!--可选的住院押金支付方式-->
  <RechargeZY>
    <银联>
      <Name>银联充值</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>

  </RechargeZY>

  <!--可选的支付方式-->
  <PayIn>
    <充值>
      <Name>诊疗卡充值</Name>
      <Visabled>1</Visabled>
      <ImageName>支付_诊疗卡充值</ImageName>
    </充值>
  </PayIn>
  <PayOut>
    <预缴金>
      <Name>诊疗卡支付</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_诊疗卡账户</ImageName>
      <Color>137,212,164</Color>
    </预缴金>
    <银联>
      <Name>银联支付</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
  </PayOut>

  <!--科室类别-->
  <RegType>
    <免费挂号>
      <Name>免费挂号</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
