    </主治医生>
    <副主任医生>
      <Name>副主任医生</Name>
      <Order>7</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_副主任医生</ImageName>
      <Color>224,102,255</Color>
