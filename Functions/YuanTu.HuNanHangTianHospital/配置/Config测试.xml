    </急诊挂号>
    <主治医师>
      <Name>主治医师 </Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_主治医师</ImageName>
      <Color>255,255,0</Color>
      <Remark>介绍信息编辑</Remark>
    </主治医师>
    <主任医师>
      <Name>主任医师</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_主任医师</ImageName>
      <Color>60,179,113</Color>
      <Remark>介绍信息编辑</Remark>
    </主任医师>
    <副主任医师>
      <Name>副主任医师</Name>
      <Order>5</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_副主任医师</ImageName>
      <Color>154,255,154</Color>
      <Remark>介绍信息编辑</Remark>
    </副主任医师>
    <主治医生>
      <Name>主治医生</Name>
      <Order>6</Order>
