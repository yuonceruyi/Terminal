    </副主任医生>
    <主任医师_外院>
      <Name>主任医师_外院</Name>
      <Order>8</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_主任医师_外院</ImageName>
      <Color>0,0,0</Color>
      <Remark>介绍信息编辑</Remark>
    </主任医师_外院>
