  <!--A6读卡器-->
  <Act_A6>
    <Port>3</Port>
    <Baud>9600</Baud>
  </Act_A6>

  <!--F3发卡器-->
  <ACT_F3>
    <Port>3</Port>
    <Baud>9600</Baud>
  </ACT_F3>

  <!--CashCode钱箱-->
  <CashCode>
    <!--CashCode钱箱串口号-->
    <Port>3</Port>
  </CashCode>

  <!--所有打印机配置-->
  <Printer>
    <!--凭条打印机名称-->
    <Receipt>MS-D347 DUSB</Receipt>
    <!--斑马打印机名称-->
    <ZBRPrinter>Zebra ZXP Series 3 USB Card Printer</ZBRPrinter>
    <!--激光打印机-->
    <Laser>PDF-XChange 5.0 for ABBYY</Laser>
  </Printer>
  <身份证>
    <端口>7</端口>
  </身份证>

  <停诊时间>
    <StartTime>20:00</StartTime>
    <EndTime>21:10</EndTime>
  </停诊时间>

  <!--斑马打印机附带射频读写模块-->
  <Icm522>
    <!--读写模块的串口值(数字)-->
    <Port>13</Port>
    <!--读写模块的串口波特率(数字)-->
    <Baud>19200</Baud>
  </Icm522>
</root>