<?xml version="1.0" encoding="utf-8" ?>
<root>
  <!--医院编号(与前置网关参数对应[唯一])-->
  <HospitalId>150</HospitalId>
  <!--操作员编号，建议每台自助机唯一-->
  <OperatorId>Z2451</OperatorId>
  <!--操作员姓名，建议每台自助机唯一-->
  <OperatorName>远图自助机</OperatorName>
  <!--院区ID-->
  <HospitalAreaCode>3306011247</HospitalAreaCode>
  <!--前置网关Url地址-->
  <GatewayUrl>http://192.168.2.53:8080/frontgateway/gateway.do</GatewayUrl>
  <!--嵊州本地socket端口-->
  <LocalSocketIP>127.0.0.1</LocalSocketIP>
  <LocalSocketPort>10001</LocalSocketPort>
  <!--监控平台Url地址-->
  <MonitorUrl>http://192.168.2.53:8080/ytmonitor/gateway.do</MonitorUrl>
  <!--健康小屋接口Url地址-->
  <HouseUrl>https://api.daily.yuantutech.com/user-web/restapi/common/health</HouseUrl>
  <!--机器型号，与自助机铭牌上一致-->
  <DeviceType>YT-540</DeviceType>

  <Assembly>YuanTu.ShengZhouZhongYiHospital</Assembly>
  <DatabasePath>Data\Database.db</DatabasePath>

  <!--组播发送的IP地址与端口-->
  <MultiCastIP>224.0.200.200</MultiCastIP>
  <MultiCastPort>8888</MultiCastPort>

  <!--是否启用语音播报引导-->
  <AudioGuideEnabled>1</AudioGuideEnabled>
  <!--是否开启转场动画-->
  <AnimationEnabled>1</AnimationEnabled>
  <!--选择主题 默认主题:default 黄蓝主题:YellowBlue-->
  <ThemeUri>YellowBlue</ThemeUri>

  <!--按钮反馈(目前支持点击后声音)-->
  <ButtonFeedBack>
    <Enable>1</Enable>
    <BeepFrequency>1000</BeepFrequency>
    <BeepDuration>200</BeepDuration>
  </ButtonFeedBack>
  <!--是否启用自动更新-->
  <AutoUpdate>0</AutoUpdate>

  <!--界面倒计时-->
  <TimeOut>60</TimeOut>
  <!--自动关机-->
  <AutoShutdown>
    <Enabled>0</Enabled>
    <Time>20:30</Time>
  </AutoShutdown>
  <!--办卡需要选择类型-->
  <SelectCreateType>1</SelectCreateType>

  <!--主页功能设置-->
  <Functions>
    <建档>
      <Name>自助建档</Name>
      <Order>0</Order>
      <Visabled>0</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_建档发卡</ImageName>
    </建档>
    <挂号>
      <Name>自助挂号</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_挂号</ImageName>
    </挂号>
    <预约>
      <Name>自助预约</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_预约</ImageName>
    </预约>
    <取号>
      <Name>自助取号</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_取号</ImageName>
    </取号>
    <缴费>
      <Name>自助缴费</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_缴费</ImageName>
    </缴费>
    <病历本打印>
      <Name>病历本打印</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_实名认证</ImageName>
    </病历本打印>
  </Functions>

  <!--查询功能-->
  <InfoQuery>
    <药品查询>
      <Name>药品信息查询</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_药品信息</ImageName>
    </药品查询>
    <项目查询>
      <Name>项目信息查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_诊疗项</ImageName>
    </项目查询>
    <已缴费明细>
      <Name>已缴费明细查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_门诊消费</ImageName>
    </已缴费明细>
    <检验结果>
      <Name>检验结果查询</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_检查报告</ImageName>
    </检验结果>
    <影像结果>
      <Name>影像结果查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_影像报告</ImageName>
    </影像结果>
    <住院一日清单>
      <Name>住院一日清单查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_住院一日清单</ImageName>
    </住院一日清单>
    <科室医生查询>
      <Name>医生查询</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>图标_检查报告</ImageName>
    </科室医生查询>
  </InfoQuery>

  <!--办卡选项-->
  <CreateType>
    <成人>
      <Name>成人</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <IsEnabled>1</IsEnabled>
      <ImageName>办卡类型_成人</ImageName>
      <Color>137,212,164</Color>
    </成人>
  </CreateType>

  <!--可使用鉴定病人信息的卡片-->
  <Card>
    <社保卡>
      <Name>社保卡</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <ImageName>卡_社保卡</ImageName>
      <建档>1</建档>
      <查询>1</查询>
    </社保卡>
  </Card>

  <!--可选的充值方式-->
  <Recharge>
    <现金>
      <Name>现金充值</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_诊疗卡账户</ImageName>
      <Color>137,212,164</Color>
    </现金>
    <银联>
      <Name>银联充值</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
    <支付宝>
      <Name>支付宝充值</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_支付宝</ImageName>
      <Color>116,206,230</Color>
    </支付宝>
    <微信支付>
      <Name>微信充值</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_微信</ImageName>
      <Color>137,212,164</Color>
    </微信支付>
  </Recharge>

  <!--可选的住院押金支付方式-->
  <RechargeZY>
    <现金>
      <Name>现金充值</Name>
      <Order>0</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_诊疗卡账户</ImageName>
      <Color>137,212,164</Color>
    </现金>
    <银联>
      <Name>银联充值</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
    <支付宝>
      <Name>支付宝充值</Name>
      <Order>2</Order>
      <Visabled>0</Visabled>
      <ImageName>支付_支付宝</ImageName>
      <Color>116,206,230</Color>
    </支付宝>
    <微信>
      <Name>微信充值</Name>
      <Order>3</Order>
      <Visabled>0</Visabled>
      <ImageName>支付_微信</ImageName>
      <Color>137,212,164</Color>
    </微信>
    <苹果>
      <Name>苹果充值</Name>
      <Order>4</Order>
      <Visabled>0</Visabled>
      <ImageName>支付_ApplePay</ImageName>
      <Color>162,169,239</Color>
    </苹果>
  </RechargeZY>

  <!--可选的支付方式-->
  <PayIn>
    <充值>
      <Name>诊疗卡充值</Name>
      <Visabled>1</Visabled>
      <ImageName>支付_诊疗卡充值</ImageName>
    </充值>
  </PayIn>
  <PayOut>
    <银联>
      <Name>银联支付</Name>
      <Order>1</Order>
      <Visabled>1</Visabled>
  <IsEnabled>1</IsEnabled>
      <ImageName>支付_银联</ImageName>
      <Color>248,180,117</Color>
    </银联>
    <支付宝>
      <Name>支付宝支付</Name>
      <Order>4</Order>
      <Visabled>1</Visabled>
  <IsEnabled>1</IsEnabled>
      <ImageName>支付_支付宝</ImageName>
      <Color>116,206,230</Color>
    </支付宝>
    <微信支付>
      <Name>微信支付</Name>
      <Order>3</Order>
      <Visabled>1</Visabled>
  <IsEnabled>1</IsEnabled>
      <ImageName>支付_微信</ImageName>
      <Color>137,212,164</Color>
    </微信支付>
    <苹果支付>
      <Name>Apple Pay</Name>
      <Order>5</Order>
      <Visabled>0</Visabled>
  <IsEnabled>1</IsEnabled>
      <ImageName>支付_ApplePay</ImageName>
      <Color>162,169,239</Color>
    </苹果支付>
  </PayOut>

  <!--科室类别-->
  <RegType>
