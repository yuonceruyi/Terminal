  <!--A6读卡器-->
  <Act_A6>
    <Port>8</Port>
    <Baud>9600</Baud>
  </Act_A6>

  <CRT310>
    <Port>1</Port>
    <Baud>9600</Baud>

  </CRT310>
  <!--F6发卡器-->
  <Act_F6>
    <Port>8</Port>
  </Act_F6>

  <身份证>
    <端口>2</端口>
  </身份证>


  <!--所有打印机配置-->
  <Printer>
    <!--凭条打印机名称-->
    <Receipt>CUSTOM K80</Receipt>
    <!--斑马打印机名称-->
    <ZBRPrinter>Zebra ZXP Series 3 USB Card Printer</ZBRPrinter>
    <!--激光打印机-->
    <Laser>Microsoft XPS Document Writer</Laser>
  </Printer>

  <!--斑马打印机附带射频读写模块-->
  <Icm522>
    <!--读写模块的串口值(数字)-->
    <Port>13</Port>
    <!--读写模块的串口波特率(数字)-->
    <Baud>19200</Baud>
  </Icm522>

  <!--金属键盘-->
  <MKeyboard>
    <Port>3</Port>
    <Baud>9600</Baud>
  </MKeyboard>



