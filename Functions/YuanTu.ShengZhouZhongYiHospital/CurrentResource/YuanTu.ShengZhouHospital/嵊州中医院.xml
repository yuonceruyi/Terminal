    <专科普通>
      <Name>专科普通</Name>
      <Order>2</Order>
      <Visabled>1</Visabled>
      <SearchDoctor>0</SearchDoctor>
      <ImageName>科室类别_专科</ImageName>
      <Color>135,206,255</Color>
      <Remark>用于紧急救治和抢救的门诊</Remark>
