			<Visabled>1</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_住院一日清单</ImageName>
		</住院一日清单>
		<执业资格查询>
			<Name>执业资格查询</Name>
			<Order>1</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_住院一日清单</ImageName>
		</执业资格查询>
        <科室医生查询>
            <Name>科室医生查询</Name>
            <Order>1</Order>
            <Visabled>1</Visabled>
            <IsEnabled>1</IsEnabled>
            <ImageName>图标_门诊消费</ImageName>
        </科室医生查询>
	</InfoQuery>

	<!--可使用鉴定病人信息的卡片-->
	<Card>
		<身份证>
			<Name>身份证</Name>
			<Order>0</Order>
			<Visabled>1</Visabled>
			<ImageName>卡_身份证</ImageName>
			<建档>1</建档>
            <查询>1</查询>
            <住院>1</住院>
		</身份证>
		<就诊卡>
			<Name>就诊卡</Name>
			<Order>1</Order>
			<Visabled>0</Visabled>
			<ImageName>卡_诊疗卡</ImageName>
			<建档>0</建档>
			<查询>1</查询>
		</就诊卡>
		<社保卡>
