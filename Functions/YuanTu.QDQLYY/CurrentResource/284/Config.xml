<?xml version="1.0" encoding="utf-8" ?>
<root>
    <maxPrintTimes>99</maxPrintTimes>
	<!--前置网关Url地址-->
	<GatewayUrl>http://192.168.8.161:8081/frontgateway/gateway.do</GatewayUrl>
	<!--监控平台Url地址-->
	<MonitorUrl>http://121.40.253.119/ytmonitor/gateway.do</MonitorUrl>
	<Assembly>YuanTu.QDQLYY</Assembly>

	<!--按钮反馈(目前支持点击后声音)-->
	<ButtonFeedBack>
		<Enable>1</Enable>
		<BeepFrequency>1000</BeepFrequency>
		<BeepDuration>200</BeepDuration>
	</ButtonFeedBack>
	<!--是否启用自动更新-->
	<AutoUpdate>0</AutoUpdate>

	<!--界面倒计时-->
	<TimeOut>60</TimeOut>
	<!--自动关机-->
	<AutoShutdown>
		<Enabled>0</Enabled>
		<Time>18:59</Time>
	</AutoShutdown>
	<!--办卡需要选择类型-->
	<SelectCreateType>1</SelectCreateType>
	<!--mp4-->
	<Clinic>
		<VideoPath>demo.mp4</VideoPath>
		<Volume>80</Volume>
	</Clinic>
	<!--广告配置-->
	<Advertisement>
		<!--广告服务地址-->
		<Url>http://10.122.2.109/aolsee-web</Url>
		<!--凭条广告位-->
		<TicketEnable>1</TicketEnable>
	</Advertisement>
	<!--主页功能设置-->
	<Functions>
		<签到>
			<Name>签到</Name>
			<Order>0</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_签到</ImageName>
		</签到>
		<建档>
			<Name>发卡</Name>
			<Order>0</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_建档发卡</ImageName>
		</建档>
		<挂号>
			<Name>挂号</Name>
			<Order>1</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_挂号</ImageName>
		</挂号>
		<预约>
			<Name>预约</Name>
			<Order>2</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_预约</ImageName>
		</预约>
		<取号>
			<Name>取号</Name>
			<Order>3</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_取号</ImageName>
		</取号>
		<缴费>
			<Name>缴费</Name>
			<Order>4</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_缴费</ImageName>
		</缴费>
		<充值>
			<Name>充值</Name>
			<Order>5</Order>
			<Visabled>0</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_充值</ImageName>
		</充值>
		<住院押金>
			<Name>住院押金</Name>
			<Order>6</Order>
			<Visabled>1</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_住院押金</ImageName>
		</住院押金>
		<查询>
			<Name>信息查询</Name>
			<Order>7</Order>
			<Visabled>1</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_查询</ImageName>
		</查询>
		<出院清单打印>
			<Name>出院清单打印</Name>
			<Order>6</Order>
			<Visabled>1</Visabled>
			<IsEnabled>1</IsEnabled>
			<ImageName>图标_住院一日清单</ImageName>
		</出院清单打印>
	</Functions>

	<!--查询功能-->
	<InfoQuery>
		<药品查询>
			<Name>药品信息查询</Name>
			<Order>0</Order>
			<Visabled>1</Visabled>
			<IsEnabled>1</IsEnabled>
