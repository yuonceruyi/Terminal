	<!--可选的住院押金支付方式-->
	<RechargeZY>
		<现金>
			<Name>现金充值</Name>
			<Order>0</Order>
			<Visabled>0</Visabled>
			<ImageName>支付_诊疗卡账户</ImageName>
			<Color>137,212,164</Color>
		</现金>
		<银联>
			<Name>银联充值</Name>
			<Order>1</Order>
			<Visabled>1</Visabled>
            <IsEnabled>1</IsEnabled>
			<ImageName>支付_银联</ImageName>
			<Color>248,180,117</Color>
		</银联>
		<支付宝>
			<Name>支付宝充值</Name>
			<Order>2</Order>
			<Visabled>1</Visabled>
            <IsEnabled>1</IsEnabled>
			<ImageName>支付_支付宝</ImageName>
			<Color>116,206,230</Color>
		</支付宝>
		<微信支付>
			<Name>微信充值</Name>
			<Order>3</Order>
            <Visabled>1</Visabled>
            <IsEnabled>1</IsEnabled>
			<ImageName>支付_微信</ImageName>
			<Color>137,212,164</Color>
		</微信支付>
		<苹果>
			<Name>苹果充值</Name>
			<Order>4</Order>
			<Visabled>0</Visabled>
			<ImageName>支付_ApplePay</ImageName>
			<Color>162,169,239</Color>
		</苹果>
	</RechargeZY>

	<!--A6读卡器-->
	<Act_A6>
		<Port>9</Port>
		<Baud>9600</Baud>
	</Act_A6>

	<!--所有打印机配置-->
	<Printer>
		<!--凭条打印机名称-->
		<Receipt>Microsoft Print to PDF</Receipt>
		<!--斑马打印机名称-->
		<ZBRPrinter>Zebra ZXP Series 3C USB Card Printer</ZBRPrinter>
		<!--激光打印机-->
        <!--<Laser>Microsoft Print to PDF</Laser>-->
		<Laser>Hewlett-Packard HP LaserJet P3010 Series</Laser>
	</Printer>

	<!--斑马打印机附带射频读写模块-->
	<Icm522>
		<!--读写模块的串口值(数字)-->
		<Port>13</Port>
		<!--读写模块的串口波特率(数字)-->
		<Baud>19200</Baud>
	</Icm522>
	<Screen>
		<Width>1080</Width>
		<Height>1920</Height>
	</Screen>
</root>