			<Order>2</Order>
			<Visabled>1</Visabled>
			<ImageName>卡_社保卡</ImageName>
			<建档>0</建档>
            <查询>1</查询>
            <住院>1</住院>
		</社保卡>
        <住院号>
            <Name>住院号</Name>
            <Order>3</Order>
            <Visabled>1</Visabled>
            <ImageName>卡_诊疗卡</ImageName>
