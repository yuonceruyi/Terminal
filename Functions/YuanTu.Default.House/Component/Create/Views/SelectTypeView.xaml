             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="900">


    <Grid
         Margin="0,80,200,30">
        <userControls:InfoMatrix  RowCount="3" ColumnCount="2" 
            ItemsSource="{Binding Data}" ItemTemplateSelector="{StaticResource InfoItemDataTemplateSelector}" />
       </Grid>
</frameworkBase:ViewsBase>

